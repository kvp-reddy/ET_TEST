<project>

    <target name="clean">
        <delete dir="build"/>
    </target>

    <target name="compile">
        <mkdir dir="build/classes"/>
        <javac srcdir="src" destdir="build/classes" />
        <classpath>
      		<fileset dir="WebContent\WEB-INF\lib">
       			 <include name="*.jar"/>
      		</fileset>
    	</classpath>
    </target>
    
</project>